{
    "name": "decodelabs/telegraph",
    "description": "Simple mailing list manager",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/relay": "^0.2",
        "decodelabs/veneer": "^0.12.8",

        "nesbot/carbon": "^3.9"
    },
    "require-dev": {
        "decodelabs/dovetail": "^0.2.14",
        "decodelabs/phpstan-decodelabs": "^0.7.0"
    },
    "conflict": {
        "decodelabs/dovetail": "<0.2"
    },
    "suggest": {
        "decodelabs/dovetail": "For dovetail config integration"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Telegraph\\": "src/Telegraph/"
        },
        "classmap": [
            "src/Dovetail/"
        ],
        "files": [
            "src/Telegraph/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
