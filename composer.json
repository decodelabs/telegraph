{
    "name": "decodelabs/telegraph",
    "description": "Simple mailing list manager",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.4",
        "decodelabs/coercion": "^0.3.5",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/kingdom": "^0.2",
        "decodelabs/nuance": "^0.1.9",
        "decodelabs/relay": "^0.2.3",

        "nesbot/carbon": "^3",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "decodelabs/commandment": "^0.3",
        "decodelabs/disciple": "^0.6",
        "decodelabs/dovetail": "^0.3.1",
        "decodelabs/phpstan-decodelabs": "^0.7.0",
        "decodelabs/stash": "^0.8",
        "decodelabs/terminus": "^0.14"
    },
    "conflict": {
        "decodelabs/commandment": "<0.3",
        "decodelabs/disciple": "<0.6",
        "decodelabs/dovetail": "<0.3",
        "decodelabs/stash": "<0.8",
        "decodelabs/terminus": "<0.14"
    },
    "suggest": {
        "decodelabs/dovetail": "For dovetail config integration"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Telegraph\\": "src/Telegraph/",
            "DecodeLabs\\Commandment\\": "src/Commandment/"
        },
        "classmap": [
            "src/Telegraph.php",
            "src/Dovetail/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
